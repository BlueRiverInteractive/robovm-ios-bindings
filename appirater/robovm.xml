<config>
	<executableName>${app.executable}</executableName>
	<mainClass>${app.mainclass}</mainClass>
	<os>ios</os>
	<arch>thumbv7</arch>
	<target>ios</target>
	<iosInfoPList>Info.plist.xml</iosInfoPList>
	<!-- Add these resources to your RoboVM project. -->
	<resources>
		<resource>
			<directory>resources</directory>
        	<includes>
        		<include>**</include>
      		</includes>
        	<skipPngCrush>true</skipPngCrush>
		</resource>
	</resources>
	<!-- Add a reference to this library to your RoboVM project. -->
	<libs>
		<lib>libs/libAppirater.a</lib>
	</libs>
	<!-- Add these frameworks to your RoboVM project. -->
	<frameworks>
		<framework>CFNetwork</framework>
		<framework>Foundation</framework>
		<framework>SystemConfiguration</framework>
	</frameworks>
	<weakFrameworks>
		<framework>StoreKit</framework>
	</weakFrameworks>
</config>